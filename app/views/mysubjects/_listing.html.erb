

<% columns = [{:type => 'html', :class => "first"}, {:type => 'html'}, {:type => 'html'}, {:type => nil, :class => "last"}] %>


<table id='mysubjects' class='display' >
  <thead>
  <tr>
    <th>Name</th>


    <th>Actions</th>
  </tr>
  </thead>
  <tbody>
  <% @mysubjects.each do |mysubject| %>
      <tr id="<%= dom_id(mysubject) %>">
        <td><%= mysubject.name %></td>


        <td><%= link_to 'Show', mysubject  %></td>
        <td><%= link_to 'Destroy', mysubject, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
  <% end %>
  </tbody>
</table>

<br />
<div class="digg_pagination">
  <%= will_paginate @mysubjects %>
</div>